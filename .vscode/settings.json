{
  "files.exclude": {
    ".trash/": true,
    ".src/": true,
    ".bak/": true,
    "**/dist/": true,
    "**/node_modules/": true,
    // The following will hide the js and map files in the editor
    "**/*.js": true,
    "**/*.map": true
  },
  "cSpell.words": [
    "EDOCMISSING",
    "Invokable",
    "Moked",
    "QLJSON",
    "chaincode",
    "couchdb",
    "easyconfig",
    "firstname",
    "hyperledger",
    "koakh",
    "lastname",
    "middlewares",
    "nestjs",
    "personid",
    "prestart",
    "worldsibu",
    "xclip"
  ],
  "workbench.colorCustomizations": {
    "activityBar.background": "#7f40bf",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#c78f58",
    "activityBarBadge.foreground": "#15202b",
    "panel.border": "#7f40bf",
    "sideBar.border": "#7f40bf",
    "editorGroup.border": "#7f40bf"
  },
  "peacock.color": "#639",
  "browser-preview.startUrl": "http://localhost:3000"
}
